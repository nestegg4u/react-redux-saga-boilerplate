{
  "name": "zanytycoon",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.5.4",
    "antd-mobile": "^2.1.11",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "moment": "^2.29.2",
    "node-less-chokidar": "^0.3.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "recompose": "^0.28.1",
